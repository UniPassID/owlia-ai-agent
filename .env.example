# Server
NODE_ENV=development
PORT=3000

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=defi_agent

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# MCP Server (Stdio)
# Example: npx -y @modelcontextprotocol/server-defi
# Or: node /path/to/your/mcp-server.js
MCP_SERVER_COMMAND=npx
MCP_SERVER_ARGS=-y,@modelcontextprotocol/server-defi

# Monitoring
MONITOR_INTERVAL_MS=300000
